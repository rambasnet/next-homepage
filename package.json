{
  "name": "next-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm install && next dev",
    "build": "npm install && next build",
    "start": "npm install && next start",
    "export": "npm install && next export",
    "deploy": "rm -rf node_modules/.cache && rimraf out && next build && next export && touch out/.nojekyll && gh-pages -d out -t true"
  },
  "dependencies": {
    "html-react-parser": "^1.2.6",
    "next": "^12.0.4",
    "nextjs": "^0.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.0",
    "gh-pages": "^3.1.0",
    "rimraf": "^3.0.2"
  }
}
